{
  "name": "express-server",
  "version": "1.0.0",
  "description": "Servidor express",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "build": "set NODE_ENV=production cd public/assets && cleancss -o main.min.css main.css",
    "dev": "nodemon index",
    "test": "mocha --exit",
    "test:cover": "nyc npm run test",
    "dev:inspect": "nodemon --inspect index",
    "dev:debug": "DEBUG=express:*,app:* npm run dev",
    "debug": "DEBUG=express:*,app:* npm run start",
    "dev:debug:windows": "set DEBUG=express:*,app:* & npm run dev",
    "debug:windows": "set DEBUG=express:*,app:* & npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianbentancourt/express-server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brianbentancourt/express-server/issues"
  },
  "homepage": "https://github.com/brianbentancourt/express-server#readme",
  "dependencies": {
    "@sentry/node": "^5.5.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "boom": "^7.3.0",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-slash": "^2.0.1",
    "helmet": "^3.20.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^3.3.0-beta2",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "pug": "^2.0.3"
  },
  "devDependencies": {
    "chalk": "^2.4.2",
    "debug": "^4.1.1",
    "mocha": "^6.2.0",
    "nodemon": "^1.18.10",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.2",
    "sinon": "^7.4.1",
    "supertest": "^4.0.2"
  },
  "nyc": {
    "all": true,
    "include": [
      "routes",
      "services",
      "lib",
      "utils"
    ]
  }
}
